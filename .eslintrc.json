{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "bracketSpacing": true,
        "trailingComma": "all",
        "singleQuote": true
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
